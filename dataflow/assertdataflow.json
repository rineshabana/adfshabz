{
	"name": "assertdataflow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Emp2",
						"type": "DatasetReference"
					},
					"name": "Employees"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "assert1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          empid as short,",
				"          name as string,",
				"          doj as string,",
				"          department as short",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Employees",
				"Employees assert(expectTrue(!isNull(toDate(doj,'yyyyMMdd')), false, 'assert1')) ~> assert1"
			]
		}
	}
}