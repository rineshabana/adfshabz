{
	"name": "Cachedataflow2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DataSetSqlTable",
						"type": "DatasetReference"
					},
					"name": "Emp"
				}
			],
			"sinks": [
				{
					"name": "CacheEmpsink"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'select max(EmpId) as maxEmpId from tbl_Employees',",
				"     format: 'query',",
				"     preferredIntegralType: 'integer') ~> Emp",
				"Emp sink(validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     store: 'cache',",
				"     format: 'inline',",
				"     output: false,",
				"     saveOrder: 1) ~> CacheEmpsink"
			]
		}
	}
}